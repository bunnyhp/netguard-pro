      "gemini-2.0-flash",
      "gemini-2.0-flash-lite",
      "learnlm-2.0-flash-experimental",
      "gemini-2.0-flash-exp"
    ],
    "groq_models": [
      "llama-3.3-70b-versatile",
      "llama-3.1-70b-versatile",
      "mixtral-8x7b-32768"
    ],
    "openrouter_models": [
